<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Визуализируй{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('css/body.css') }}">
            <link rel="stylesheet" href="{{ asset('jquery-ui-1.11.4/jquery-ui.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('images/logo.png') }}"/>

        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.12.0.min.js') }}"></script>
            <script src="{{ asset('js/handlebars-v4.0.5.js') }}"></script>
            <script src="{{ asset('jquery-ui-1.11.4/jquery-ui.js') }}"></script>
            <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </head>

    <body>
        <div class="container">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
